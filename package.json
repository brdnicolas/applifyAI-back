{
  "name": "tenstack-starter",
  "version": "2.0.0",
  "description": "Typescript Node Express Rest API server starter",
  "private": true,
  "authors": "filoscoder",
  "license": "MIT",
  "scripts": {
    "lint": "eslint 'src/**/*.{ts,js}'",
    "transpile": "tsc",
    "clean": "rimraf dist",
    "build": "npx tsc && yarn swagger",
    "start": "NODE_ENV=production node dist/index.js",
    "watch": "tsc --watch",
    "dev": "NODE_ENV=development nodemon",
    "swagger": "ts-node ./src/config/swagger.ts",
    "service:start": "pm2 start ecosystem.config.js",
    "service:reload": "pm2 reload ecosystem.config.js",
    "service:startup": "pm2 startup",
    "service:stop": "pm2 stop ecosystem.config.js",
    "service:list": "pm2 list ecosystem.config.js",
    "service:delete": "pm2 delete ecosystem.config.js",
    "service:logs": "pm2 logs"
  },
  "keywords": [
    "typescript",
    "node",
    "express",
    "rest",
    "api",
    "server"
  ],
  "dependencies": {
    "@types/cheerio": "^0.22.35",
    "@types/morgan": "^1.9.9",
    "@types/swagger-jsdoc": "^6.0.4",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "axios": "^1.6.3",
    "bcrypt": "^5.1.1",
    "cheerio": "^1.0.0-rc.12",
    "cors": "^2.8.3",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "pg": "^8.11.3",
    "prettier": "^3.1.1",
    "prettier-eslint": "^16.2.0",
    "rimraf": "^3.0.2",
    "sequelize": "^6.35.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.1",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.11",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/module-alias": "^2.0.1",
    "@types/node": "^20.10.5",
    "@types/swagger-ui-express": "^4.1.6",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-eslint-plugin": "^5.2.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.5.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.1.1",
    "eslint-plugin-promise": "^6.1.1",
    "module-alias": "^2.2.3",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "swagger-autogen": "^2.23.7",
    "ts-node": "^10.2.0",
    "tsc": "^2.0.3",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">= 16.0.0"
  }
}
